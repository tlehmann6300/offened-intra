{
    "name": "ibc/backend",
    "description": "IBC Backend with PHPMailer and Dotenv",
    "type": "project",
    "require": {
        "php": ">=7.4",
        "phpmailer/phpmailer": "^6.9",
        "vlucas/phpdotenv": "^5.6"
    },
    "autoload": {
    "classmap": [
        "config/",
        "src/"
    ],
    "psr-4": {
        "App\\": "src/"
    }
},
    "config": {
        "optimize-autoloader": true
    }
}
